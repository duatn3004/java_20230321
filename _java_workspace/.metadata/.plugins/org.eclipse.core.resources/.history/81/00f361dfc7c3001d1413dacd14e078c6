package day15;

public class word {
	/*단어장 프로그램 만들기
	Word class
	단어, 의미를 저장하는 클래스
	word,mean
	apple:사과
	car:자동차
	banana:바나나*/
	private String word;
	private String mean;
	
	public word() {}

	public word(String word, String mean) {
		this.word = word;
		this.mean = mean;
	}

	public String getWord() {
		return word;
	}

	public void setWord(String word) {
		this.word = word;
	}

	public String getMean() {
		return mean;
	}

	public void setMean(String mean) {
		this.mean = mean;
	}

	@Override
	public String toString() {
		return word+":"+mean;
	}
	
	
	

}
