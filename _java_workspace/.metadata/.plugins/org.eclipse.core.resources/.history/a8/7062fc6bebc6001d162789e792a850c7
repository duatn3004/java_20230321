package day17;

public class WordProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
class wordprogram {
	
	private String word;
	private String mean;
	
	public wordprogram() {}
	
	public wordprogram(String word, String mean) {
		this.word = word;
		this.mean = mean;
	}

	public String getWord() {
		return word;
	}

	public void setWord(String word) {
		this.word = word;
	}

	public String getMean() {
		return mean;
	}

	public void setMean(String mean) {
		this.mean = mean;
	}

	@Override
	public String toString() {
		return "wordprogram [word=" + word + ", mean=" + mean + "]";
	}
	
	
}

